Flags
{
    "targetname"
    {
        display : "Entity Name"
        type    : "string"
        default : ""
    }
    "StartDisabled"
    {
        display : "Start Disabled"
        type    : "bool"
        default : "0"
    }
    "TeamNum"
    {
        display : "Team"
        type    : "team"
        default : "0"
    }
    "angles"
    {
        type : "int_angles"
        default : "0 0 0"
    }
    "respawnroomname"
    {
        display : "Assoc. Respawn Room Name"
        type    : "string"
        default : ""
    }
    
